		"svelte-check": "^4.0.0",
		"sveltekit-sprite": "^0.1.2",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@castlenine/svelte-qrcode": "^2.3.0",
